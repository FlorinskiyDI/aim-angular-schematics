{
  "name": "@aim/aws-deploy",
  "version": "0.0.3",
  "scripts": {
    "build": "tsc -p tsconfig.schematics.json",
    "postbuild": "copyfiles --all \"schematics/collection.json\" \"schematics/main/*/schema.json\" \"schematics/{configs,main}/*/files/**\" \"schematics/components/**/*\" ../dist/library"
  },
  "ng-add": {
    "save": "devDependencies"
  },
  "schematics": "./schematics/collection.json",
  "peerDependencies": {
    "@angular/common": "^19.1.0",
    "@angular/core": "^19.1.0"
  },
  "dependencies": {
    "tslib": "^2.3.0"
  },
  "sideEffects": false,
  "devDependencies": {
    "copyfiles": "^2.4.1"
  }
}
